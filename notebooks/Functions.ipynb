   "source": [
    "def addnumber(x,n=5):\n",
    "    y = []\n",
    "    for i in x:\n",
    "        y.append(i+n)\n",
    "    return y\n",
    "\n",
    "a = range(3)\n",
    "print(addnumber(a))\n",
    "print(addnumber(a,n=2))"
