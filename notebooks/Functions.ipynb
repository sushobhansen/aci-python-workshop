   "source": [
    "## Add 5 to a list"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def add5(x):\n",
    "    y = []\n",
    "    for i in x:\n",
    "        y.append(i+5)\n",
    "    return y\n",
    "\n",
    "a = ['one','two']\n",
    "b = add5(a)\n",
    "print(b)"
   ]
  },
